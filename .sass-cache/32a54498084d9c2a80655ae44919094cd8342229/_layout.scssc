/* Grid-2 vars */

$width-1: 50% !default; 
$width-2: 50% !default; 
$margin-r: 5% !default; 

@mixin ui-grid-2($width-1,$width-2,$margin-r) {
	@include pie-clearfix;

	> .ui-block-1,
	> .ui-block.first {
		width: $width-1 - ($margin-r/2);
		@include float-left;
		margin-right: $margin-r;
	}

	> .ui-block-2,	
	> .ui-block.second {
		width: $width-2 - ($margin-r/2);
		@include float-left;
		margin-right: 0;
	}
}

