/* ============================================= */
/*	Universal						   			 */ 
/* ============================================= */

a 		{ 	text-decoration: none; color: inherit; }
strong	{	font-weight: bold;	}
em   	{	font-style: italic;	}

a {
	color: $site-spot;
	@include trans;

	&:hover {
		@include trans;
		color: $site-black;
	}
}

body {
	overflow: hidden;
}

h1,h2,h3,h4,h5 {
	-webkit-font-smoothing: antialiased;
}

h3 {
	margin: 0 auto;
	margin-bottom: 15px;
	color: #333332;
	max-width: $standard-width;
	font: 700 27px/140% $sans-title;

	@media (min-width: 700px) {
		font-size: 30px;
	}
}

h4 {
	font: 400 22px/140% $sans-title;	
	text-transform: uppercase;
	letter-spacing: 2px;
}

.overline {
	margin: 0 auto;
	max-width: $standard-width;
	text-transform: uppercase;
	letter-spacing: 3px;
	font: 400 10px/170% $sans-title;
	color: $site-spot;
	margin-bottom: 8px;
	-webkit-font-smoothing: antialiased;

	@media (min-width: 600px) {
		font-size: 11px;
	}//mq
}

.intro-overline {
	margin: 0 auto;
	text-transform: uppercase;
	letter-spacing: 3px;
	font: 400 10px/170% $sans-title;
	color: $site-spot;
	margin-bottom: 8px;
	-webkit-font-smoothing: antialiased;
	text-align: center;

	@media (min-width: 600px) {
		font-size: 11px;
	}//mq
}

p {
	-webkit-font-smoothing: antialiased;
}

.site-list {
	max-width: $standard-width;
	margin: 0 auto;
	font: 400 16px/170% $sans-title;

	li {
		margin-bottom: 10px;
	}
}

.npr-list {
	width: 75%;
	margin-right: 0px;

	@media (min-width: 600px) {
		display: inline-block;
		margin-right: 52px;
		max-width: 500px;
		width: 44%;
		vertical-align: top;
	}
}

.npr-list.last {
	margin-right: 0px;
}
 
/* ============================================= */
/*	Header							   			 */ 
/* ============================================= */

header {
	background-color: #fff;
	position: relative;
	z-index: 4;
	height: 65px;
	position: fixed;
	width: 100%;

	@media (min-width: 600px) {
		padding: 22px 42px;
	}//mq

	@media (min-width: 760px) {
		@include opacity (0.9);
	}

	@media (min-width: 900px) {
		padding: 22px 72px;

	}//mq
}

.nav-chrome {
	height: 65px;
	width: 100%;
	padding: 22px 10px;
	position: fixed;
	top: 0px;
	left: 0px;
	z-index: 4;
	background-color: #fff;

	@media (min-width: 760px) {
		padding: 22px 0px;
		z-index: 2;
		left: auto;
	}
}

.nav-logo-txt {
	display: inline;
	float: left;
	font: 600 17px/140%;
	text-transform: uppercase;
	letter-spacing: 1px;
	text-indent: -20000px;	
}

.nav-logo-mod {
	float: left;

	@media (min-width: 760px) {
