/* ============================================= */
/*	Universal						   			 */ 
/* ============================================= */

a 		{ 	text-decoration: none; color: inherit; }
strong	{	font-weight: bold;	}
em   	{	font-style: italic;	}

a {
	color: $site-spot;
	@include trans;

	&:hover {
		@include trans;
		color: $site-black;
	}
}

body {
	overflow: hidden;
}

h1,h2,h3,h4,h5 {
	-webkit-font-smoothing: antialiased;
}

h1 {
	margin: 0 auto;
	font: 400 45px/120% $serif;
	max-width: $standard-width;
	margin-bottom: 16px;
	color: #333332
}

h3 {
	margin: 0 auto;
	margin-bottom: 15px;
	color: #333332;
	max-width: $standard-width;
	font: 700 27px/140% $sans-title;
}

h4 {
	font: 400 22px/140% $sans-title;	
	text-transform: uppercase;
	letter-spacing: 2px;
}

.overline {
	margin: 0 auto;
	max-width: $standard-width;
	text-transform: uppercase;
	letter-spacing: 3px;
	font: 400 10px/170% $sans-title;
	color: $site-spot;
	margin-bottom: 8px;
	-webkit-font-smoothing: antialiased;

	@media (min-width: 600px) {
		font-size: 11px;
	}//mq
}

.intro-overline {
	margin: 0 auto;
	text-transform: uppercase;
	letter-spacing: 3px;
	font: 400 10px/170% $sans-title;
	color: $site-spot;
	margin-bottom: 8px;
	-webkit-font-smoothing: antialiased;
	text-align: center;

	@media (min-width: 600px) {
		font-size: 11px;
	}//mq
}

p {
	-webkit-font-smoothing: antialiased;
}

.site-list {
	max-width: $standard-width;
	margin: 0 auto;
	font: 400 16px/170% $sans-title;

	li {
		margin-bottom: 10px;
	}
}

.npr-list {
	width: 75%;
	margin-right: 0px;

	@media (min-width: 600px) {
		display: inline-block;
		margin-right: 52px;
		max-width: 500px;
		width: 44%;
		vertical-align: top;
	}
}

.npr-list.last {
	margin-right: 0px;
