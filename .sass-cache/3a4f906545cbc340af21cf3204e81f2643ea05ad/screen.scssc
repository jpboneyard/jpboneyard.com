/* ============================================= */
/*	Home										 */ 
/* ============================================= */

.home-intro {
	background-color: #fff;
	margin: 0 auto;
	padding: 30px 0px; 

	h2 {
		margin: 0 auto;
		text-align: center;
		font: 400 21px/140% $sans-title;
		text-transform: uppercase;
		letter-spacing: 2px;
	}
}




/* ============================================= */
/*	Featured Project							 */ 
/* ============================================= */

.feat-project-mod {
	@include pie-clearfix;
	position: relative;
	height: 0;
	padding-bottom: 74.5%;
	overflow: hidden;
	z-index: -1;

	@media (min-width: 400px) {
		padding-bottom: 67.5%;
	}

	@media (min-width: 600px) {
		padding-bottom: 60.5%;
	}

	@media (min-width: 1000px) {
		padding-bottom: 56.5%;
	}

	@media (min-width: 1200px) {
		padding-bottom: 48.5%;
	}

	@media (min-width: 1400px) {
		padding-bottom: 42.5%;
	}

	img {
		width: 100%;
	}
}

.feat-project-img {
	position: fixed;
	top: 70px;
	right: 0;
	left: 0;
	width: 110%;

	/*@media (min-width: 1400px) {
		top: -70px;
	}*/
}


/* ============================================= */
/*	Project Tiles								 */ 
/* ============================================= */

.projects-mod {
	padding: 15px 15px 62px 15px;
	@include pie-clearfix;
	margin: 0 auto;
	text-align: center;
	// background-color: #FAFBFB;
	background-color: #f7f7f7;
	position: relative;

	@media (min-width: 500px) {
		padding: 32px 32px 62px 32px;
	}//mq	

	@media (min-width: 900px) {
		padding: 62px 72px 72px 72px;
	}//mq

	@media (min-width: 1300px) {
		padding: 62px 102px 72px 102px;
	}
}

.inside {
	padding-top: 82px;
}

.inside-projects {
	.projects-mod {
		padding-top: 82px;
	}
}

.project-tiles {
	margin: 0 auto;
	@include pie-clearfix;

	@media (min-width:600px) and (max-width: 1000px) {
		@include media-grid(2, 20px);
    }

	@media (min-width: 1000px) {
		@include media-grid(3, 20px);
    }

}

.tile-detail-mod {
	position: absolute;
	bottom: 0px;
	padding: 17% 5%;
	background-color: #fff;
	@include pie-clearfix;
	width: 100%;
	height: 100%;
	@include trans(.7s, opacity);
	@include opacity(0);

	@media (min-width: 500px) {
		width: calc(100% - 19px);
	}
}

.tile-details {
	margin: 0 auto;
	text-align: center;
	font: 800 13px/140% $sans-title;

	h4 {
		font-size: 14px;

		@media (min-width:800px) {
			font-size: 19px;
		}//mq
	}
}

.grid-item {
	display: block;
	position: relative;
	width: 95%;
	margin: 0 auto 19px;
	@include opacity(1);
	> a {
		display: block;
		}
	img {
		width: 100%;
		}
	&:hover {
		.tile-detail-mod {
			@include opacity(1);
			@include trans(.2s, opacity);	
		}
	}	
}

.grid-item .top {
	z-index: 10;
	position: absolute;
	top: 0;
	left: 0;
	@include trans(.7s, opacity);
}

.more-work-mod {
	margin: 0 auto;
	text-align: center;
	margin-bottom: -45px;
	background-color: #fff;

	img {
		max-width: 92px;
		position: relative;
		z-index: 3;
	}
}


/* ============================================= */
/*	Project	Page								 */ 
/* ============================================= */

.project-page-mod{
	background-color: #fff;
}

.project-page-content {
	background-color: #fff;
	padding: 32px 15px;
	margin: 0 auto;

	@media (min-width:500px) {
		padding: 52px 52px;
	}

	img {
		
	}

	p {
		margin: 0 auto;
		margin-bottom: 30px;
		color: #333332;;
		font: 400 17px/180% $serif;
		max-width: $standard-width;

		@media (min-width: 500px) {
			font-size: 21px;
		}

		&:last-child {
			margin-bottom: 0px;
		}

		&:first-child {
			margin-bottom: 22px;
		}
	}

}

.img-set {
	@include media-grid(2, 50px);
}

.img-sm {
	max-width: $standard-width;
	display: block;
	margin: 0 auto;
	width: 100%;
	margin-bottom: 30px;
}

.img-med {
	max-width: 1200px;
	display: block;
	margin: 0 auto;
	width: 100%;
	margin-bottom: 30px;
}

.img-full {
	width: 110%;
	margin-left: -5%;
	margin-bottom: 30px;
}

.full-m {
	width: 140%;
	margin-left: -20%;

	@media (min-width: 600px) {
		width: 110%;
		margin-left: -5%;
	}
}

.img-first {
	margin-top: 30px;
}

.img-ftr-top {
	margin-bottom: -100px;
}

.section-end {
	margin-bottom: 45px;

	@media (min-width: 700px) {
		margin-bottom: 60px;
	}
}

.section-start {
	margin-top: 45px;

	@media (min-width: 700px) {
		margin-top: 60px;
	}
}

.site-width-img {
	width: 100%;
	max-width: none;
}

.site-width-mod {
	max-width: 200%;
}

.img-site-width {
	max-width: 200%;

	img {
		width: 200%;
	}
}

.project-details {
	max-width: $standard-width;
	width: 100%;
	display: block;
	margin: 0 auto;
}

/* ============================================= */
/*	Footer										 */ 
/* ============================================= */

.ftr-lead-mod {
	background-color: #FAFBFB;
	padding: 52px;
	text-align: center;
}

footer {
	background-color: #fff;
	height: 200px;
	position: relative;
	margin-top: -60px;
}

.ftr-logo-mod {
	margin: 0 auto;
	text-align: center;
	margin-top: -40px;
	position: relative;
	z-index: 3;

	img {
		max-width: 92px;
	}
}

.social-list {
	margin: 0px 0px 20px 0px;
	text-align: center;
	padding-top: 70px;
}

.social-item {
	display: inline;
	margin-right: 15px;
	@include opacity(.7);
	@include trans;

	&:last-child {
		margin-right: 0px;
	}//lc

	&:hover {
		@include trans;
		@include opacity(1);
	}//hover
}//social-item

.social-item-img {
	max-width: 27px;
	width: 100%;
}

.copyright {
	text-transform: uppercase;
	letter-spacing: 1px;
	font-size: 8px;
	text-align: center;	
	color: #222;
}

/* ============================================= */
/*	Print Styles							     
	Inline to avoid required HTTP connection     */
/* ============================================= */

// Import Print Styles
@import "project/print";



