	}

	h2 {
		margin: 0 auto;
		text-align: center;
		font: 400 23px/136% $serif;
		max-width: 860px;
		margin-bottom: 10px;

		a {
			color: #333332;

			&:hover { 
				color: $site-spot;
			}//hover
		}//a

		@media (min-width: 600px) {
			font-size: 41px;
			line-height: 125%;
		}//mq
	}//h2

	p {
		font: 400 20px/150% $sans-title;
