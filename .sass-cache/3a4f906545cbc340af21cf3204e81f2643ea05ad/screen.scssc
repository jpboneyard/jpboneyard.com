/* ============================================= */

.tile-list-mod {
	padding: 62px 15px 62px 15px;
	@include pie-clearfix;
	margin: 0 auto;
	text-align: center;
	background-color: #f7f7f7;
	position: relative;

	@media (min-width: 500px) {
		padding: 62px 32px 62px 32px;
