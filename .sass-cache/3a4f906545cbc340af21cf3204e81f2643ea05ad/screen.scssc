/* ============================================= */
/*	Universal						   			 */ 
/* ============================================= */

a 		{ 	text-decoration: none; color: inherit; }
strong	{	font-weight: bold;	}
em   	{	font-style: italic;	}

a {
	color: $site-spot;
	@include trans;

	&:hover {
		@include trans;
		color: $site-black;
	}
}

body {
	overflow: hidden;
}

h1,h2,h3,h4,h5 {
	-webkit-font-smoothing: antialiased;
}

h3 {
	margin: 0 auto;
	margin-bottom: 15px;
	color: #333332;
	max-width: $standard-width;
	font: 700 27px/140% $sans-title;

	@media (min-width: 700px) {
		font-size: 30px;
	}
}

h4 {
	font: 400 22px/140% $sans-title;	
	text-transform: uppercase;
	letter-spacing: 2px;
}

.overline {
	margin: 0 auto;
	max-width: $standard-width;
	text-transform: uppercase;
	letter-spacing: 3px;
	font: 400 10px/170% $sans-title;
	color: $site-spot;
	margin-bottom: 8px;
	-webkit-font-smoothing: antialiased;

	@media (min-width: 600px) {
		font-size: 11px;
	}//mq
}

.intro-overline {
	margin: 0 auto;
