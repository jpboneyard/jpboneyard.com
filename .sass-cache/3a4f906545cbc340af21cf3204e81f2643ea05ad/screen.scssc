/* ============================================= */
/*	Universal						   			 */ 
/* ============================================= */

a 		{ 	text-decoration: none; color: inherit; }
strong	{	font-weight: bold;	}
em   	{	font-style: italic;	}

a {

}

body {
	overflow: hidden;
}

h1,h2,h3,h4,h5 {
	-webkit-font-smoothing: antialiased;
}

h1 {
	margin: 0 auto;
	font: 400 34px/120% $serif;
	max-width: $standard-width;
	margin-bottom: 16px;
	color: #333332;

	@media (min-width: 500px) {
		font-size: 45px;
	}//mq
}

h3 {
	margin: 0 auto;
	margin-bottom: 6px;
	color: #333;
	max-width: $standard-width;
	font: 700 20px/140% $sans-title;
	text-transform: uppercase;
	letter-spacing: 1px;

	@media (min-width: 500px) {
		font: 700 24px/140% $sans-title;
	}//mq

}

h4 {
	font: 400 22px/140% $sans-title;	
	text-transform: uppercase;
	letter-spacing: 2px;
}

.overline {
	margin: 0 auto;
	max-width: $standard-width;
	text-transform: uppercase;
	letter-spacing: 3px;
	font: 400 10px/170% $sans-title;
	color: $site-spot;
	margin-bottom: 8px;
	-webkit-font-smoothing: antialiased;

	@media (min-width: 500px) {
		font-size: 11px;
	}//mq
}

.intro-overline {
	margin: 0 auto;
	text-transform: uppercase;
	letter-spacing: 3px;
	font: 400 10px/170% $sans-title;
	color: $site-spot;
	margin-bottom: 8px;
	-webkit-font-smoothing: antialiased;
	text-align: center;

	@media (min-width: 600px) {
		font-size: 11px;
	}//mq
}

p {
	-webkit-font-smoothing: antialiased;

	a {
		color: $site-spot;
		@include trans;

		&:hover {
			@include trans;
			color: $site-black;
		}//hover
	}//a
}//p

li {
	a {
		color: $site-spot;
		@include trans;

		&:hover {
			@include trans;
			color: $site-black;
		}//hover
	}//a
}//li

.site-list {
	max-width: $standard-width;
	margin: 0 auto;
	font: 400 16px/170% $sans-title;

	li {
		margin-bottom: 10px;
	}
}

.npr-list {
	width: 75%;
	margin-right: 0px;

	@media (min-width: 600px) {
		display: inline-block;
		margin-right: 52px;
		max-width: 500px;
		width: 44%;
		vertical-align: top;
	}
}

.npr-list.last {
	margin-right: 0px;
}

 
/* ============================================= */
/*	Header							   			 */ 
/* ============================================= */

header {
	background-color: #fff;
	position: relative;
	z-index: 4;
	height: 65px;
	position: fixed;
	width: 100%;

	@media (min-width: 760px) {
		@include opacity (0.9);
	}

	@media (min-width: 900px) {
		padding: 22px 72px;

	}//mq
}

.nav-chrome {
	height: 65px;
	width: 100%;
	padding: 22px 22px;
	position: fixed;
	top: 0px;
	left: 0px;
	z-index: 4;
	background-color: #fff;

	@media (min-width: 760px) {
		padding: 22px px;
		z-index: 2;
		left: auto;
	}
}

.nav-logo-mod {
	float: left;

	@media (min-width: 760px) {
		padding-top: 0px;
	}

	img {
		max-width: 140px;
		width: 100%;

		@media (min-width: 760px) {
			max-width: 150px;
		}//mq
	}//img
}

.nav-title {
	float: left;
	font: 200 1px/0%;
	max-width: 1px;
	text-indent: -20000px;	
}

.nav-link-mod {
	float: right;
	display: block;
	margin-top: -300px;

	@media (min-width: 760px) {
		padding-top: 3px;
		margin-top: 0px;
	}
}

.nav-link-item {
	display: inline;
	margin-right: 20px;
	font: 400 13px/140% $sans-title;
	@include trans;
	text-transform: uppercase;
	letter-spacing: 2px;

	@media (min-width: 760px) {
		padding: 0px;
	}

	a {
		&:hover { 
			color: $site-black;
			@include trans;
			background-color: #ddd;

			@media (min-width: 760px) {
				background-color: #fff;
			}//mq
		}//hover
	}//a

	&:last-child {
		margin-right: 0px;
	}
}


/***  Mobile-Nav  ***/ 

.m-nav-toggle {
	max-width: 30px;
	width: 100%;
	padding-top: 17px;
	float: right;
	display: block;
	background-image: url(/_img/_icons/m-nav-icon.png);
	background-size: 25px;
	margin-top: 4px;

	@media (min-width: 760px) {
		display: none;
	}//mq
}

.visible {
	display: block;
	background-color: #F2f2f2;
	width: 100%;
	position: relative;
	z-index: 1;
	margin-top: 65px;
	-webkit-transition: margin-top 0.3s ease-in-out;
	-moz-transition: margin-top 0.3s ease-in-out;
	-o-transition: margin-top 0.3s ease-in-out;
	transition: margin-top 0.3s ease-in-out;

	.nav-link-item {
		display: block;
		border-bottom: 1px solid #ddd;
		margin-right: 0px;

		&:last-child {
			border-bottom: 0px;
		}//lc

		a {
			padding-left: 10px;
			display: block;
			padding: 30px 0px 30px 10px;
		}//a
	}//nav-link-item
}


#access {
	float: none;
	background-color: #000;
	position: relative;
	z-index: 2;

	@media (min-width: 760px) and (max-width: 900px) {
			padding: 20px 20px 0px 0px;

	}

	@media (min-width: 760px) {
		float: right;
		background-color: transparent;		
	}//mq

	#menu-main-nav {
		> li {
			display: block;
			border-bottom: 1px solid;
			border-color: #222;

			@media (min-width: 760px) {
				display: inline;
				border-bottom: 0px solid;
			}//mq

			a {
				@include inline-block;
				background-color: #262626;
				color: #fff;
				font: 600 15px/120% ;
				text-transform: uppercase;
				letter-spacing: 1px;
				margin-right: 0px;
				display: block;
				padding: 22px 6px 22px 20px;
				@include trans;

				&:last-child {
					margin-right: 0px;
					border-bottom: 0px;
				}

				&:hover {
					@include trans;
					color: red;
					background-color: #333;
				}//hover

				@media (min-width: 760px) {
					display: inline;
					margin-right: 3px;
					background-color: #000;
					padding: 2px 8px;
					border-bottom: 0px;

					&:hover {
						background-color: #121212;
					}//hover
				}//mq
			}//a	
		}//li
	}//menu-main-nav
}//#access 


