}

.tile-details {
	position: absolute;
	background: #fff;
	width: 100%;
	height: 100%;
	padding: 16% 2%;
	left: 0;
	bottom: 0;
	letter-spacing: 1px;
	text-transform: uppercase;
	text-align: center;
	font: 800 13px/140% $sans-title;
	@include opacity(0);
	@include trans(.7s, opacity);
	border: 1px solid #fff;

