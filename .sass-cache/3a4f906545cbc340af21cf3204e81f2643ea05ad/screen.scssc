a {
	color: $site-spot;
	@include trans;

	&:hover {
		@include trans;
		color: $site-black;
	}
}

body {
	overflow: hidden;
}

h1,h2,h3,h4,h5 {
	-webkit-font-smoothing: antialiased;
}

h1 {
	margin: 0 auto;
	font: 400 45px/120% $serif;
	max-width: $standard-width;
	margin-bottom: 16px;
	color: #333332
}

h3 {
	margin: 0 auto;
	margin-bottom: 15px;
	color: #333332;
	max-width: $standard-width;
	font: 700 27px/140% $sans-title;
}

h4 {
	font: 400 22px/140% $sans-title;	
	text-transform: uppercase;
	letter-spacing: 2px;
}

.overline {
	margin: 0 auto;
	max-width: $standard-width;
