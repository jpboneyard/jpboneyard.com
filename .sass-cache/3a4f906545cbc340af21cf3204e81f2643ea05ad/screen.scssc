@import "partials/base";

/* ## Base CSS */
// @import "project/type";
// @import "project/forms";
// @import "project/tables";
// @import "project/lists";

/* ## Components */
// @import "project/buttons";
// @import "project/dropdowns";
// @import "project/tabs";
// @import "project/modals";
// @import "project/tooltips";


  
/* ============================================= */
/*	Universal						   			 */ 
/* ============================================= */

a 		{ 	text-decoration: none; color: inherit; }
strong	{	font-weight: bold;	}
em   	{	font-style: italic;	}

body {
	overflow: hidden;
}

h1,h2,h3,h4,h5 {
	-webkit-font-smoothing: antialiased;
}

h3 {
	margin: 0 auto;
	margin-bottom: 15px;
	color: #333332;
	max-width: $standard-width;
	font: 700 27px/140% $sans-title;

	@media (min-width: 700px) {
		font-size: 30px;
	}
}

h4 {
	font: 400 22px/140% $sans-title;	
	text-transform: uppercase;
	letter-spacing: 2px;
}

.overline {
	margin: 0 auto;
	max-width: $standard-width;
	text-transform: uppercase;
	letter-spacing: 3px;
	font: 400 10px/170% $sans-title;
	color: $site-spot;
	margin-bottom: 8px;
	-webkit-font-smoothing: antialiased;

	@media (min-width: 600px) {
		font-size: 11px;
	}//mq
}

p {
	-webkit-font-smoothing: antialiased;
}

.site-list {
	max-width: $standard-width;
	margin: 0 auto;
	font: 400 18px/170% $sans-title;

	li {
		margin-bottom: 10px;
	}
}
 
/* ============================================= */
/*	Header							   			 */ 
/* ============================================= */

header {
	background-color: #fff;
	position: relative;
	z-index: 4;

	@media (min-width: 600px) {
		padding: 22px 42px;
	}//mq

	@media (min-width: 760px) {
		@include opacity (0.9);
	}

	@media (min-width: 900px) {
		padding: 22px 72px;

	}//mq

	height: 70px;
	position: fixed;
	width: 100%;
}

.nav-chrome {
	height: 70px;
	width: 100%;
	padding: 22px 10px;
	position: fixed;
	top: 0px;
	z-index: 2;

	@media (min-width: 760px) {
		padding: 22px 0px;
	}
}

.nav-logo-txt {
	display: inline;
	float: left;
	font: 600 17px/140%;
	text-transform: uppercase;
	letter-spacing: 1px;
	text-indent: -20000px;	
}

.nav-logo-mod {
	float: left;

	@media (min-width: 760px) {
		padding-top: 0px;
	}

	img {
		max-width: 140px;
		width: 100%;

		@media (min-width: 760px) {
			max-width: 160px;
		}
	}
}

.nav-link-mod {
	float: right;
	display: none;

	@media (min-width: 760px) {
		display: block;
		padding-top: 3px;
	}
}

.nav-link-item {
	display: inline;
	margin-right: 20px;
	font: 800 13px/140% $sans-title;
	@include trans;
	text-transform: uppercase;
	letter-spacing: 2px;

	@media (min-width: 760px) {
		padding: 0px;
	}

	a {
		&:hover { 
			color: #22ABA1;
			@include trans;
			background-color: #ddd;

			@media (min-width: 760px) {
				background-color: #fff;
			}//mq
		}//hover
	}//a

	&:last-child {
		margin-right: 0px;
	}
}


/***  Mobile-Nav  ***/ 

.m-nav-toggle {
	max-width: 30px;
	width: 100%;
	padding-top: 17px;
	float: right;
	display: block;
	background-image: url(/_img/_icons/m-nav-icon.png);
	background-size: 25px;
	margin-top: 4px;

	@media (min-width: 760px) {
		display: none;
	}//mq
}

.visible {
	display: block;
	background-color: #F2f2f2;
	width: 100%;
	position: relative;
	z-index: 1;
	top: 70px;

	.nav-link-item {
		display: block;
		border-bottom: 1px solid #ddd;
		margin-right: 0px;

		&:last-child {
			border-bottom: 0px;
		}//lc

		a {
			padding-left: 10px;
			display: block;
			padding: 30px 0px 30px 10px;
		}//a
	}//nav-link-item
}


#access {
	float: none;
	background-color: #000;
	position: relative;
	z-index: 2;

	@media (min-width: 760px) {
		float: right;
		background-color: transparent;		
	}//mq

	#menu-main-nav {
		> li {
			display: block;
			border-bottom: 1px solid;
			border-color: #222;

			@media (min-width: 760px) {
				display: inline;
				border-bottom: 0px solid;
			}//mq

			a {
				@include inline-block;
				background-color: #262626;
				color: #fff;
				font: 600 15px/120% ;
				text-transform: uppercase;
				letter-spacing: 1px;
				margin-right: 0px;
				display: block;
				padding: 22px 6px 22px 20px;
				@include trans;

				&:last-child {
					margin-right: 0px;
					border-bottom: 0px;
				}

				&:hover {
					@include trans;
					color: red;
					background-color: #333;
				}//hover

				@media (min-width: 760px) {
					display: inline;
					margin-right: 3px;
					background-color: #000;
					padding: 2px 8px;
					border-bottom: 0px;

					&:hover {
						background-color: #121212;
					}//hover
				}//mq
			}//a	
		}//li
	}//menu-main-nav
}//#access 

