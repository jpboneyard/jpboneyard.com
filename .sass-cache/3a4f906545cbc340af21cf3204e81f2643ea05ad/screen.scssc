	}//mq

	img {
		max-width: 75px;
		position: relative;
		z-index: 3;

		@media (min-width: 760px) {
			max-width: 92px;
		}//mq

	}//img
}//more-work-mod


.tile-mod {

	&:hover .tile-details {
			@include trans(.2s, opacity);
			@include opacity(1);
	}//hover
}

.shows-mod {
	margin: 0 auto;
	@include pie-clearfix;
}

.tile-list {

	@media (min-width: 501px) and (max-width: 1100px){		
		@include media-grid(2, 20px);
	}//mq

	@media (min-width: 1100px) {
		@include media-grid(3, 20px);
		margin: 0 auto;
	}//mq
}

.tile-item {
	margin: 0 auto;
	text-align: center;
	margin-bottom: 20px;
	position: relative;
	overflow: hidden;
}

.show-img-sm {
	width: 100%;
}

.tile-details {
	position: absolute;
	background: #fff;
	width: 100%;
	height: 100%;
	padding: 18% 2%;
	left: 0;
	bottom: 0;
	letter-spacing: 1px;
	text-transform: uppercase;
	text-align: center;
	font: 800 13px/140% $sans-title;
	@include opacity(0);
	@include trans(.7s, opacity);
	border: 1px solid #fff;

	.overline {
		margin-bottom: 0px;
	}

	h4 {
		font-size: 17px;

		@media (min-width: 800px) {
			font-size: 20px;
		}//mq

		@media (min-width: 1300px) {
			font-size: 23px;
		}//mq
	}//h4
}


.tile-1 {
	animation: fadein .9s;	
	-webkit-animation:fadein .9s;		
}

.tile-2 {
	animation: fadein 1.8s;	
	-webkit-animation:fadein 1.8s;		
}

.tile-3 {
	animation: fadein 2.7s;	
	-webkit-animation:fadein 2.7s;		
}

.tile-4 {
	animation: fadein 5.4s;	
	-webkit-animation:fadein 5.4s;		
}

.tile-5 {
	animation: fadein 4.5s;	
	-webkit-animation:fadein 4.5s;		
}

.tile-6 {
	animation: fadein 3.6s;	
	-webkit-animation:fadein 3.6s;	
}

.tile-7 {
	animation: fadein 6.3s;	
	-webkit-animation:fadein 6.3s;		
}

.tile-8 {
	animation: fadein 7.2s;	
	-webkit-animation:fadein 7.2s;	
}

.tile-9 {
	animation: fadein 8.1s;	
	-webkit-animation:fadein 8.1s;
	margin-left: 0px;

	@media (min-width: 501px) {
		margin-left: 25%;
	}

	@media (min-width: 1100px) {
		margin-left: 0px;
	}	
}//tile-9


.tile-1, .tile-2, .tile-3, .tile-4, .tile-5, .tile-6, .tile-7, .tile-8, .tile-9 {
	@media (min-width: 10px) and (max-width: 1000px) {
		animation: fadein .1s;	
		-webkit-animation:fadein .1s;	
	}//mq
}

@-webkit-keyframes fadein {
  0%   { @include opacity(0); }
  50%   { @include opacity(0); } 
  100% { @include opacity(1); }
}

@-moz-keyframes fadein {
  0%   { @include opacity(0); }
  50%   { @include opacity(0); } 
  100% { @include opacity(1); }
}  

.inside-projects {
	.tile-1,.tile-2,.tile-3,.tile-4,.tile-5,.tile-6,.tile-7,.tile-8,.tile-9 {
		animation: fadein .1s;	
		-webkit-animation:fadein .1s;
	}
}






