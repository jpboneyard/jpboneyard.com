.computron-topper {
	position: absolute;
	bottom: 0px;
	z-index: 20;
}

.home-topper {
	position: relative;
	top: 65px;
	right: 0;
	left: 0;
	width: 110%;
}

.home-intro-slide {
	-webkit-animation: slidedown 3s linear;
	-moz-animation: slidedown 3s linear;
	-o-animation: slidedown 3s linear;
}

@-webkit-keyframes slidedown {
	0% {margin-top: -1000px;}
	100% {margin-top: 0px;}
}
@-moz-keyframes slidedown {
	0% {margin-top: -1000px;}
	100% {margin-top: 0px;}
}
@-o-keyframes slidedown {
	0% {margin-top: -1000px;}
	100% {margin-top: 0px;}
}

